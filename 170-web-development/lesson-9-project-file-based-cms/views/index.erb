<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
  <% if session[:message] %>
    <%= session.delete(:message) %>
  <% end %>
  <ul>
    <% @filenames.each do |filename| %>
      <li>
        <a href="<%= filename %>"><%= filename %></a>
        <a href="<%= filename %>/edit">Edit</a>
      </li>
    <% end %>
  </ul>
</body> 
</html>